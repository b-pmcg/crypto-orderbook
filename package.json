    "d3-scale": "^2.1.0",
    "d3-selection": "^1.3.0",
    "express": "^4.16.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "signalr-client": "0.0.17",
    "signalrjs": "^0.2.2",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
